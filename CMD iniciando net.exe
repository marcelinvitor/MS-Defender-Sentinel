// Autor: Marcelo Rodrigues
//Busca pelo uso do net.exe indicando possíveis manipulações de usuários e permissões.
DeviceProcessEvents
| where FileName == "net.exe"
| where ProcessCommandLine matches regex @"\b(net\s+(user|group|localgroup|accounts|view|use))\b"
| project Timestamp, DeviceName, InitiatingProcessAccountName, InitiatingProcessFileName, ProcessCommandLine, InitiatingProcessVersionInfoCompanyName, InitiatingProcessSHA256
| order by Timestamp desc
